{
    "swagger": "2.0",
    "info": {
        "description": "API used for executing SQL QUERY",
        "title": "API GATEWAY SQL",
        "contact": {
            "name": "API Support",
            "email": "ngaswilly77@gmail.com"
        },
        "license": {
            "name": "MIT",
            "url": "https://github.com/willbrid/easy_api_prom_sms_alert/blob/main/LICENSE"
        },
        "version": "1.0.0"
    },
    "basePath": "/v1",
    "paths": {
        "/api-gateway-sql/stats": {
            "get": {
                "security": [
                    {
                        "BasicAuth": []
                    }
                ],
                "description": "Get all batch statistics",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "apisql"
                ],
                "summary": "Get statistics",
                "parameters": [
                    {
                        "type": "integer",
                        "default": 1,
                        "description": "Page number",
                        "name": "page_num",
                        "in": "query"
                    },
                    {
                        "type": "integer",
                        "default": 20,
                        "description": "Page size",
                        "name": "page_size",
                        "in": "query"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/httputil.HTTPResp"
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "$ref": "#/definitions/httputil.HTTPResp"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "$ref": "#/definitions/httputil.HTTPResp"
                        }
                    }
                }
            }
        },
        "/api-gateway-sql/{datasource}/init": {
            "post": {
                "security": [
                    {
                        "BasicAuth": []
                    }
                ],
                "description": "Initialize Database by providing a sql query file",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "apisql"
                ],
                "summary": "Initialize Database",
                "parameters": [
                    {
                        "type": "string",
                        "description": "Datasource Name",
                        "name": "datasource",
                        "in": "path",
                        "required": true
                    },
                    {
                        "type": "file",
                        "description": "SQL Data to upload",
                        "name": "sqlfile",
                        "in": "formData",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/httputil.HTTPResp"
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "$ref": "#/definitions/httputil.HTTPResp"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "$ref": "#/definitions/httputil.HTTPResp"
                        }
                    }
                }
            }
        },
        "/api-gateway-sql/{target}": {
            "get": {
                "security": [
                    {
                        "BasicAuth": []
                    }
                ],
                "description": "Trigger SQL query without params",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "apisql"
                ],
                "summary": "Get SQL Query result",
                "parameters": [
                    {
                        "type": "string",
                        "description": "Target Name",
                        "name": "target",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/httputil.HTTPResp"
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "$ref": "#/definitions/httputil.HTTPResp"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "$ref": "#/definitions/httputil.HTTPResp"
                        }
                    }
                }
            },
            "post": {
                "security": [
                    {
                        "BasicAuth": []
                    }
                ],
                "description": "Trigger SQL query with params",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "apisql"
                ],
                "summary": "Get SQL Query result",
                "parameters": [
                    {
                        "type": "string",
                        "description": "Target Name",
                        "name": "target",
                        "in": "path",
                        "required": true
                    },
                    {
                        "description": "Data to send",
                        "name": "data",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/httputil.HTTPResp"
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "$ref": "#/definitions/httputil.HTTPResp"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "$ref": "#/definitions/httputil.HTTPResp"
                        }
                    }
                }
            }
        },
        "/api-gateway-sql/{target}/batch": {
            "post": {
                "security": [
                    {
                        "BasicAuth": []
                    }
                ],
                "description": "Execute batch sql query with values from a csv file",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "apisql"
                ],
                "summary": "Execute batch sql query",
                "parameters": [
                    {
                        "type": "string",
                        "description": "Target Name",
                        "name": "target",
                        "in": "path",
                        "required": true
                    },
                    {
                        "type": "file",
                        "description": "CSV Data to import",
                        "name": "csvfile",
                        "in": "formData",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/httputil.HTTPResp"
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "$ref": "#/definitions/httputil.HTTPResp"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "$ref": "#/definitions/httputil.HTTPResp"
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "httputil.HTTPResp": {
            "type": "object",
            "properties": {
                "code": {
                    "type": "integer",
                    "example": 200
                },
                "data": {},
                "message": {
                    "type": "string",
                    "example": "status ok"
                }
            }
        }
    },
    "securityDefinitions": {
        "BasicAuth": {
            "type": "basic"
        }
    }
}